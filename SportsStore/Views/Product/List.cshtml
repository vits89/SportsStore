@model ProductsViewModel

@foreach (var product in Model.Products)
{
    <partial name="ProductSummary" model="@product" />
}
<ul class="pagination" page-model="@Model.PagingInfo" page-action="List" page-item-class="page-item"
    page-item-class-active="active"
    page-link-class="page-link"
    page-url-category="@Model.CurrentCategory"></ul>
